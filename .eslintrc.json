{
  "env": {
      "browser": true,
      "es6": true,
      "mocha": true
  },
  "parserOptions": {
    "ecmaFeatures": {
        "experimentalObjectRestSpread": true
    },
    "sourceType": "module"
},
  "extends":  ["airbnb"],
  "rules": {
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "comma-dangle": ["error", "only-multiline"],
    "no-underscore-dangle": 0,
    "no-console": ["error", { "allow": ["error"] }]
}
}
